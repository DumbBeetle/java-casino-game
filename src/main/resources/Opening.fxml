<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="opening" onKeyPressed="#keyPressed" prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.OpController">
   <children>
      <ImageView fx:id="background" disable="true" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Opening.png" />
         </image>
      </ImageView>
      <ImageView fx:id="newGame" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Buttons/NewGame.png" />
         </image>
      </ImageView>
      <ImageView fx:id="loadGame" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Buttons/LoadGame.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
